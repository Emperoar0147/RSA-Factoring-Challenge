#!/usr/bin/python3

import sys


def is_prime(num):
    if num <= 1:
        return False
    if num <= 3:
        return True
    if num % 2 == 0 or num % 3 == 0:
        return False
    i = 5
    while i * i <= num:
        if num % i == 0 or num % (i + 2) == 0:
            return False
        i += 6
    return True


def factorize_rsa(number):
    for i in range(2, int(number**0.5) + 1):
        if number % i == 0 and is_prime(i) and is_prime(number // i):
            return f"{number}={i}*{number//i}"
    return "No prime factors found"


def main(filename):
    with open(filename, 'r') as file:
        rsa_number = int(file.read().strip())

    factorization = factorize_rsa(rsa_number)
    print(factorization)


if __name__ == "__main__":
    if len(sys.argv) != 2:
        print("Usage: ./factors_rsa <file>")
        sys.exit(1)

    main(sys.argv[1])
