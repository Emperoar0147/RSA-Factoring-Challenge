#!/usr/bin/python3

import sys


def factorize_rsa_number(n):
    # Find the smallest prime factor of n
    for i in range(2, int(n**0.5) + 1):
        if n % i == 0:
            return i, n // i
    return "Unable to factorize, the number might be prime."


def main(filename):
    with open(filename, 'r') as file:
        n = int(file.read().strip())

    p, q = factorize_rsa_number(n)
    print(f"{n}={p}*{q}")


if __name__ == "__main__":
    if len(sys.argv) != 2:
        print("Usage: ./rsa_factors <file>")
        sys.exit(1)

    main(sys.argv[1])
