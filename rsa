#!/usr/bin/python3

import sys
import math


def is_prime(num):
    if num <= 1:
        return False
    if num <= 3:
        return True
    if num % 2 == 0 or num % 3 == 0:
        return False
    i = 5
    while i * i <= num:
        if num % i == 0 or num % (i + 2) == 0:
            return False
        i += 6
    return True


def factorize_rsa(n):
    for i in range(2, int(math.sqrt(n)) + 1):
        if n % i == 0 and is_prime(i) and is_prime(n // i):
            return i, n // i


def main(filename):
    with open(filename, 'r') as file:
        n = int(file.readline().strip())

    p, q = factorize_rsa(n)
    print(f"{n}={p}*{q}")


if __name__ == "__main__":
    if len(sys.argv) != 2:
        print("Usage: ./rsa_factors <file>")
        sys.exit(1)

    main(sys.argv[1])
