#!/usr/bin/python3

import sys
import sympy

def find_prime_factors(n):
    """
    Finds the prime factors of a given number n.
    
    Args:
    - n: The number to factorize
    
    Returns:
    - p, q: The prime factors of n
    """
    p, q = sympy.factorint(n)
    return p, q

if __name__ == "__main__":
    if len(sys.argv) != 2:
        print("Usage: python script.py <RSA_number>")
        sys.exit(1)
    
    n = int(sys.argv[1])
    p, q = find_prime_factors(n)
    print(f"The prime factors of {n} are: {p} and {q}")
