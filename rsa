#!/usr/bin/python3

import sys
from sympy import isprime

def factorize_rsa_number(n):
    for p in range(2, int(n**0.5) + 1):
        if n % p == 0:
            q = n // p
            if isprime(p) and isprime(q):
                return p, q
    return None, None

if __name__ == "__main__":
    if len(sys.argv) != 2:
        print("Usage: python script.py <RSA_number>")
        sys.exit(1)
    
    n = int(sys.argv[1])
    p, q = factorize_rsa_number(n)
    if p is None or q is None:
        print("Unable to find prime factors for the given RSA number.")
    else:
        print("Prime factors of", n, "are:", p, "and", q)
